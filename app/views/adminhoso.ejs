<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>
    <div>
        <table>
            <table class="table table-hover">
                <thead>
                    <tr>
                        <th scope="col">stt</th>
                        <th scope="col">name</th>
                        <th scope="col">số đt </th>
                        <th scope="col">tiền vay</th>
                        <th scope="col">thời hạn vay</th>
                       
                    </tr>
                </thead>
                <tbody>
                    <% for(var i = 0; i < user.length; i++) { %>
                        <tr>
                            <th scope="row"><%= user[i].id %><th>
                            <td scope="col"><%= user[i].name %><td>
                            <td scope="col"><%= user[i].phone %> <td>
                            <td scope="col"><%= user[i].money %> vnd<td>
                            <td scope="col"><%= user[i].time %> tháng<td>
                        </tr>
                     
                      <% } %>
                   
                
                </tbody>
            </table>
        </table>
    </div>
</body>

</html>